{
  "name": "pgraph",
  "version": "0.0.1",
  "description": "Property Graph Exchange Format",
  "license": "MIT",
  "devDependencies": {
    "@peggyjs/eslint-parser": "^2.0.6",
    "@peggyjs/eslint-plugin": "^2.1.2",
    "ajv": "^8.12.0",
    "chai": "^5.0.0",
    "eslint": "^8.57.0",
    "mocha": "^10.2.0",
    "neo4j-driver-lite": "^5.17.0",
    "peggy": "^4.0.0",
    "peggy-tracks": "^3.2.0",
    "pre-commit": "^1.2.2"
  },
  "main": "index.js",
  "type": "module",
  "bin": {
    "pgraph": "./bin/pgraph.js"
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint --fix './**/*.js' && eslint './**/*.pegjs'",
    "update-example": "pgraph examples/example.pg -t csv examples/csv/example",
    "parser": "peggy --format es -o src/parser/pg.js ./src/pg.pegjs",
    "peggy": "peggy",
    "peggy-tracks": "peggy-tracks",
    "docs": "./docs/make.sh",
    "build": "npm run parser && npm test && npm run docs"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "@pinemach/csv": "^1.0.4",
    "dotparser": "^1.1.1"
  }
}
